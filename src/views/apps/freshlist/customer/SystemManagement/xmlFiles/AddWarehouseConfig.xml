    <name>WarehouseName</name>
    <placeholder>Enter WarehouseName</placeholder>
	</input>

    <input>
	<label>Mobile No.</label>
	<type type="number">text</type>
    <name>mobileno</name>
    <placeholder>Enter Mobile No.</placeholder>
	</input>

	<input>
	<label>Email</label>
	<type type="email">email</type>
