<CreateTarget>
	<MyDropDown>
		<dropdown>
			<label>Product List</label>
			<name>ProductName</name>
			<option value="001">001</option>
			<option value="002">002</option>
			<option value="003">003</option>
		</dropdown>
	</MyDropDown>
	<MyDropDown>
		<dropdown>
			<label>Party List</label>
			<name>PartyName</name>
			<option value="001">001</option>
			<option value="002">002</option>
			<option value="003">003</option>
		</dropdown>
	</MyDropDown>

	<MyDropDown>
		<dropdown>
			<label>Unit</label>
			<name>Unit</name>
			<option value="001">001</option>
			<option value="002">002</option>
			<option value="003">003</option>
		</dropdown>
